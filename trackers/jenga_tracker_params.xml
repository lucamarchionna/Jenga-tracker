<?xml version="1.0"?>
<conf>
  <ecm>
    <mask>
      <size>5</size>
      <nb_mask>180</nb_mask>
    </mask>
    <range>
      <tracking>8</tracking>
    </range>
    <contrast>
      <edge_threshold>10000</edge_threshold>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
    </contrast>
    <sample>
      <step>2</step>
    </sample>
  </ecm>
  <klt>
    <mask_border>5</mask_border> 
    <max_features>300</max_features> 
    <window_size>5</window_size> 
    <quality>0.015</quality> 
    <min_distance>8</min_distance> 
    <harris>0.01</harris>
    <size_block>3</size_block> 
    <pyramid_lvl>3</pyramid_lvl> 
  </klt>
  <depth_dense>
    <sampling_step>
      <step_X>4</step_X>
      <step_Y>4</step_Y>
    </sampling_step>
  </depth_dense>  
  <camera>
    <!-- If tracker-live, parameters are read from intrinsic file instead -->
    <!--Projection model type-->
    <type>perspectiveProjWithDistortion</type>
    <!--Pixel ratio-->
    <px>598.3646185934283</px>
    <py>600.84171062438497</py>
    <!--Principal point-->
    <u0>327.30636184296856</u0>
    <v0>245.57599534161491</v0>
    <!--Undistorted to distorted distortion parameter-->
    <kud>0.089107796430835037</kud>
    <!--Distorted to undistorted distortion parameter-->
    <kdu>-0.08649131805040268</kdu>
  </camera>
  <face>
    <angle_appear>85</angle_appear> 
    <angle_disappear>90</angle_disappear> 
    <near_clipping>0.01</near_clipping>
    <far_clipping>1.5</far_clipping>
    <fov_clipping>0</fov_clipping>
  </face>
  <lod>
    <!-- General lod parameters, may be overwritten by .cao model -->
    <use_lod>0</use_lod>
    <min_line_length_threshold>100</min_line_length_threshold>
    <min_polygon_area_threshold>300</min_polygon_area_threshold>
  </lod>
</conf>

